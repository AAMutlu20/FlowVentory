      <_ContentIncludedByDefault Remove="FlowVentory.DL\obj\project.packagespec.json" />
      <_ContentIncludedByDefault Remove="FlowVentory.BLL\bin\Debug\net8.0\FlowVentory.BLL.deps.json" />
      <_ContentIncludedByDefault Remove="FlowVentory.BLL\bin\Debug\net8.0\FlowVentory.BLL.runtimeconfig.json" />
      <_ContentIncludedByDefault Remove="FlowVentory.BLL\obj\Debug\net8.0\FlowVentory.BLL.sourcelink.json" />
